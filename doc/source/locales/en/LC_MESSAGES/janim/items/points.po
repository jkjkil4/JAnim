#: janim.items.points.NamedGroup:6 of
msgid ""
"也可以使用 :meth:`~.NamedGroupMixin.add` 或 :meth:`~.NamedGroupMixin.insert` "
"方法，传入具名参数来新增具名子物件"
msgstr ""
"You can also use :meth:`~.NamedGroupMixin.add` or "
":meth:`~.NamedGroupMixin.insert` and pass keyword arguments to add named "
"child items"

#: janim.items.points.NamedGroup:8 of
msgid "示例："
msgstr "Example:"

#: janim.items.points.NamedGroup:10 of
msgid ""
"group = NamedGroup(\n"
"    text=Text('lorem'),\n"
"    shape=Circle()\n"
")\n"
"group.points.arrange(DOWN, aligned_edge=LEFT)\n"
"\n"
"self.play(\n"
"    group['text'].anim.color.set(GREEN),\n"
"    group['shape'].anim.color.set(YELLOW),\n"
"    lag_ratio=0.5\n"
")\n"
"\n"
"def updater(group: NamedGroup, p: UpdaterParams) -> None:\n"
"    group.points.rotate(TAU * p.alpha)\n"
"    group['text'].color.mix(BLUE, p.alpha)\n"
"\n"
"self.play(\n"
"    GroupUpdater(group, updater)\n"
")"
msgstr ""
"group = NamedGroup(\n"
"    text=Text('lorem'),\n"
"    shape=Circle()\n"
")\n"
"group.points.arrange(DOWN, aligned_edge=LEFT)\n"
"\n"
"self.play(\n"
"    group['text'].anim.color.set(GREEN),\n"
"    group['shape'].anim.color.set(YELLOW),\n"
"    lag_ratio=0.5\n"
")\n"
"\n"
"def updater(group: NamedGroup, p: UpdaterParams) -> None:\n"
"    group.points.rotate(TAU * p.alpha)\n"
"    group['text'].color.mix(BLUE, p.alpha)\n"
"\n"
"self.play(\n"
"    GroupUpdater(group, updater)\n"
")"

