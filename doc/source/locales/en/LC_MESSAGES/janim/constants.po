#: ../../source/janim/constants.rst:57
msgid "数学常数"
msgstr "Mathematical Constants"

#: ../../source/janim/constants.rst:59
msgid ""
"PI = np.pi\n"
"TAU = 2 * PI\n"
"DEGREES = TAU / 360\n"
"# Nice to have a constant for readability\n"
"# when juxtaposed with expressions like 30 * DEGREES\n"
"RADIANS = 1"
msgstr ""
"PI = np.pi\n"
"TAU = 2 * PI\n"
"DEGREES = TAU / 360\n"
"# Nice to have a constant for readability\n"
"# when juxtaposed with expressions like 30 * DEGREES\n"
"RADIANS = 1"

