#: ../../source/janim/constants.rst:18
msgid "坐标"
msgstr "Coordinates"

#: ../../source/janim/constants.rst:20
msgid "JAnim 使用三维坐标，并且用 ``ndarray`` 的类型"
msgstr "JAnim uses three-dimensional coordinates and the ``ndarray`` type"

#: ../../source/janim/constants.rst:22
msgid ""
"ORIGIN = np.array((0., 0., 0.))\n"
"UP = np.array((0., 1., 0.))\n"
"DOWN = np.array((0., -1., 0.))\n"
"RIGHT = np.array((1., 0., 0.))\n"
"LEFT = np.array((-1., 0., 0.))\n"
"IN = np.array((0., 0., -1.))\n"
"OUT = np.array((0., 0., 1.))\n"
"X_AXIS = np.array((1., 0., 0.))\n"
"Y_AXIS = np.array((0., 1., 0.))\n"
"Z_AXIS = np.array((0., 0., 1.))\n"
"\n"
"NAN_POINT = np.full(3, np.nan)\n"
"\n"
"# Useful abbreviations for diagonals\n"
"UL = UP + LEFT\n"
"UR = UP + RIGHT\n"
"DL = DOWN + LEFT\n"
"DR = DOWN + RIGHT"
msgstr ""
"ORIGIN = np.array((0., 0., 0.))\n"
"UP = np.array((0., 1., 0.))\n"
"DOWN = np.array((0., -1., 0.))\n"
"RIGHT = np.array((1., 0., 0.))\n"
"LEFT = np.array((-1., 0., 0.))\n"
"IN = np.array((0., 0., -1.))\n"
"OUT = np.array((0., 0., 1.))\n"
"X_AXIS = np.array((1., 0., 0.))\n"
"Y_AXIS = np.array((0., 1., 0.))\n"
"Z_AXIS = np.array((0., 0., 1.))\n"
"\n"
"NAN_POINT = np.full(3, np.nan)\n"
"\n"
"# Useful abbreviations for diagonals\n"
"UL = UP + LEFT\n"
"UR = UP + RIGHT\n"
"DL = DOWN + LEFT\n"
"DR = DOWN + RIGHT"

