"        self.forward(0.3)"

#: ../../source/janim/anims/movement.rst:20
msgid ""
"from janim.imports import *\n"
"\n"
"class FollowExample(Timeline):\n"
"    def construct(self):\n"
"        dot = Dot(RIGHT * 2).show()\n"
"        txt = Text('dot').show()\n"
"        txt.points.next_to(dot, DOWN)\n"
"\n"
"        self.forward()\n"
"        self.play(\n"
"            Succession(\n"
"                Rotate(dot, PI * 3 / 2, about_point=ORIGIN),\n"
"                dot.anim.points.shift(UP * 4),\n"
"                duration=3\n"
"            ),\n"
"            Follow(txt, dot, DOWN, duration=3),\n"
"        )\n"
"        self.forward()"
msgstr ""
"from janim.imports import *\n"
"\n"
"class FollowExample(Timeline):\n"
"    def construct(self):\n"
"        dot = Dot(RIGHT * 2).show()\n"
"        txt = Text('dot').show()\n"
"        txt.points.next_to(dot, DOWN)\n"
"\n"
"        self.forward()\n"
"        self.play(\n"
"            Succession(\n"
"                Rotate(dot, PI * 3 / 2, about_point=ORIGIN),\n"
"                dot.anim.points.shift(UP * 4),\n"
"                duration=3\n"
"            ),\n"
"            Follow(txt, dot, DOWN, duration=3),\n"
"        )\n"
"        self.forward()"
