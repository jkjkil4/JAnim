"\n"
"self('select: txt[0]').set(color=RED)\n"
"self('select: txt[1]').set(color=GREEN)"

#: ../../source/tutorials/gui_commands.rst:47
msgid "``camera`` 命令"
msgstr "``camera`` Command"

#: ../../source/tutorials/gui_commands.rst:49
msgid "自由转动/移动/缩放摄像机视角"
msgstr "Freely rotate/move/zoom the camera view"

#: ../../source/tutorials/gui_commands.rst:53
#, python-brace-format
msgid ""
"ThreeDAxes(\n"
"    axis_config={\n"
"        'include_tip': True\n"
"    }\n"
").show()\n"
"\n"
"circle = Circle().show()\n"
"circle.points.rotate(40 * DEGREES, axis=UP)\n"
"\n"
"self('camera')"
msgstr ""
"ThreeDAxes(\n"
"    axis_config={\n"
"        'include_tip': True\n"
"    }\n"
").show()\n"
"\n"
"circle = Circle().show()\n"
"circle.points.rotate(40 * DEGREES, axis=UP)\n"
"\n"
"self('camera')"

#: ../../source/tutorials/gui_commands.rst:66
msgid ""
"NumberPlane(faded_line_ratio=1).show()\n"
"\n"
"square1 = Square(color=RED, fill_alpha=1)\n"
"square2 = Square(color=GREEN, fill_alpha=1)\n"
"square3 = Square(color=BLUE, fill_alpha=1)\n"
"\n"
"square2.points.rotate(PI / 2, axis=UP)\n"
"square3.points.rotate(PI / 2, axis=RIGHT)\n"
"\n"
"squares = Group(square1, square2, square3)\n"
"squares.show().apply_depth_test()\n"
"\n"
"self('camera')"
msgstr ""
"NumberPlane(faded_line_ratio=1).show()\n"
"\n"
"square1 = Square(color=RED, fill_alpha=1)\n"
"square2 = Square(color=GREEN, fill_alpha=1)\n"
"square3 = Square(color=BLUE, fill_alpha=1)\n"
"\n"
"square2.points.rotate(PI / 2, axis=UP)\n"
"square3.points.rotate(PI / 2, axis=RIGHT)\n"
"\n"
"squares = Group(square1, square2, square3)\n"
"squares.show().apply_depth_test()\n"
"\n"
"self('camera')"

