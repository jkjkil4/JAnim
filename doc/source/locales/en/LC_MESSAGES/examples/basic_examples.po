"            vec2 uv = v_texcoord;\n"
"\n"
"            float glitchStrength = sin(time) * 0.02;\n"
"            vec2 offset = vec2(glitchStrength, 0.0);\n"
"\n"
"            float r = texture(fbo, uv + offset).r;\n"
"            float g = texture(fbo, uv).g;\n"
