"from janim.imports import *\n"
"\n"
"class FrameEffectExample(Timeline):\n"
"    def construct(self):\n"
"        squares = Square(0.5, color=BLUE, fill_alpha=0.3) * 49\n"
"        squares.points.arrange_in_grid()\n"
"\n"
"        effect1 = SimpleFrameEffect(    # (2~8s) [::2] 的方块产生渐变色\n"
"            squares[::2],\n"
"            shader='''\n"
